                    <div class="form1 col-md-12 card py-3">
                      <div class="form-group">
                        <%= form.label :name %>
                        <%= form.text_field :name, class: 'form-control', required: true %>
                      </div>
                      <div class="form-group">
                        <%= form.label :Applied_To%>
                        <%= form.select :entity_type, ['CAMPAIGN','ADSET','AD'],{},{ class: 'form-control'} %>
