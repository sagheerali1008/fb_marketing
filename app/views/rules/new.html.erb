                        </div>
                        <div class="form-group">
                          <%= form.label :operator%>
                          <%= form.select :operator,['GREATER_THAN','LESS_THAN','EQUAL'] , {}, {class: 'form-control'}%>
                        </div>
                        <div class="form-group">
                          <%= form.label :time_preset%>
                          <%= form.select :time_preset, ['LAST_2_DAYS','LAST_3_DAYS','LAST_7_DAYS','LAST_28_DAYS','LAST_30_DAYS'],{}, {class: 'form-control'} %>
                        </div>
                      </div>
                    </div>
                  </div>
